<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="movieeee" autowire="byName" class="com.stackroute.domain.Movie"></bean>

    <bean id="actor" class="com.stackroute.domain.Actor">
        <property name="name" value="Anushka"></property>
        <property name="gender" value="Female"></property>
        <property name="age" value="32"></property>
    </bean>

    <bean id="movie1" autowire="constructor" class="com.stackroute.domain.Movie"></bean>

    <bean id="actor1" class="com.stackroute.domain.Actor">
        <constructor-arg value="Anushka"></constructor-arg>
        <constructor-arg value="Female"></constructor-arg>
        <constructor-arg value="32"></constructor-arg>
    </bean>
    <bean id="BeanLifecycle" class="com.stackroute.demo.BeanLifeCycleDemoBean" init-method="customInit" destroy-method="customDestroy"></bean>
    <bean class="com.stackroute.demo.BeanPostProcessorDemoBeanImpl"></bean>

</beans>